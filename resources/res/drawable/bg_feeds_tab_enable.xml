<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#f2a900"/>
        </shape>
    </item>
    <item android:left="1.5dp">
        <shape android:shape="rectangle">
            <solid android:color="#222527"/>
        </shape>
    </item>
    <item android:left="2.5dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="0.5dp"
                android:color="#f7f9fa"/>
            <gradient
                android:startColor="#9fc7cc"
                android:endColor="#d6e2e9"
                android:angle="0"
                android:type="linear"
                android:centerX="0.5"
                android:centerY="0.5"
                android:centerColor="#cfe4f2"/>
        </shape>
    </item>
</layer-list>
